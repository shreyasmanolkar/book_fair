 {{#each data.bookInfo}}

 <div class="display-b">
        <div class="book-section-one">
            <div class=" b-book-image">
                <div class="b-img">
                    <img  class="b-img" src={{image_url}} alt="book-img">
                </div>
            </div>
            <div class="b-detail">
                <h2>{{name}}</h2>
                <h5>Seller: <a href="/sellers/{{seller_id}}">{{full_name}}</a></h5><br>
                <h2>â‚¹  {{price}}</h2><br>
                <h1> Book Id: {{book_id}}</h1><br>
                <form action="/seller/delete" method="POST" class="check">
                    <input type="number" name="bookId" id="book_id" placeholder="Enter book id listed above" value={{bookId}}><br><br>
                    <input type="submit" value="Delete Book" class="btn">
                </form>
            </div>
        </div>
        <div class="book-section-two">
            <h4>Description:</h4><br>
            <p>{{description}}</p>
            </div>
</div>

{{/each}}